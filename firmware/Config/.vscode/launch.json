{
    "version": "0.2.0",
    "configurations": [{
        "name": "Debug (OpenOCD)",
        "type": "cppdbg",
        "cwd": "${workspaceFolder}",
        "request": "launch",
        "program": "${workspaceFolder}/Build/main.elf",
        "args": [],
        "preLaunchTask": "build",
        "serverLaunchTimeout": 20000,
        "environment": [],
        "externalConsole": false,
        "serverStarted": "target halted due to debug-request, current mode: Thread",
        "debugServerArgs": "-f ${workspaceFolder}/../Config/stlink.cfg -f ${workspaceFolder}/../Config/stm32f1x.cfg -c init -c \"reset init\"",
        "filterStderr": true,
        "filterStdout": false,
        "logging": {
            "moduleLoad": true,
            "trace": true,
            "engineLogging": true,
            "programOutput": true,
            "exceptions": true
        },
        "osx": {
            "MIMode": "gdb",
            "MIDebuggerPath": "/usr/local/opt/arm-none-eabi-gcc/bin/arm-none-eabi-gdb",
            "debugServerPath": "/usr/local/bin/openocd"
        },
        "setupCommands": [
            { "text": "-file-exec-and-symbols ${workspaceRoot}/Build/main.elf", "description": "load file", "ignoreFailures": false},
            { "text": "target extended-remote localhost:3333", "ignoreFailures": false },
            { "text": "monitor reset halt", "ignoreFailures": false },
            { "text": "-target-download", "description": "flash target", "ignoreFailures": false }
        ],
    }]
  }