{
    "configurations": [{
            "name": "STM32 Debug",
            "compilerPath": "/usr/local/bin/arm-none-eabi-gcc",
            "includePath": [
                "${workspaceFolder}/../Inc",
                "${workspaceFolder}/Inc/**",
                "${workspaceFolder}/Drivers/**"
            ],
            "defines": [
                "DEBUG=1",
                "_DEBUG",
                "GCC_PATH=/usr/local/bin/arm-none-eabi-gcc/bin",
                "UNICODE",
                "_UNICODE",
                "USE_HAL_DRIVER",
                "STM32F103xE"  
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}